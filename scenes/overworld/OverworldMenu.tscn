code = "shader_type canvas_item;

varying flat vec4 modulate;
void vertex() {
	modulate = COLOR;
}

void fragment(){
    vec2 newuv = UV;
    newuv.x += TIME;
    newuv.y += TIME;
    vec4 c = texture(TEXTURE, newuv);
    COLOR = c * modulate;
}"

